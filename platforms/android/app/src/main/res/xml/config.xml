<?xml version='1.0' encoding='utf-8'?>
<widget id="com.app.gestionDeClientes" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="InAppBrowser">
        <param name="android-package" value="org.apache.cordova.inappbrowser.InAppBrowser" />
    </feature>
    <feature name="NetworkStatus">
        <param name="android-package" value="org.apache.cordova.networkinformation.NetworkManager" />
    </feature>
    <feature name="Printer">
        <param name="android-package" value="de.appplant.cordova.plugin.printer.Printer" />
    </feature>
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Exit">
        <param name="android-package" value="im.ckk.cordova.exit.Exit" />
    </feature>
    <feature name="Notification">
        <param name="android-package" value="org.apache.cordova.dialogs.Notification" />
    </feature>
    <feature name="Toaster">
        <param name="android-package" value="Toaster" />
    </feature>
    <name>GestionDeClientes</name>
    <description>
        Una aplicación que te ayudará a gestionar de una manera efectiva, intuitiva y facil tus clientes
    </description>
    <author email="josedanielmolina323@gmail.com" href="">
        Jose Daniel Molina
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <icon density="mdpi" platform="android" src="www/images/icon.png" />
    <preference name="loglevel" value="DEBUG" />
</widget>
